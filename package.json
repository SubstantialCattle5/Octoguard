    "test": "node -e \"require('./.github/actions/low-effort/index.js'); require('./.github/actions/ai-suspect/index.js'); require('./.github/actions/auto-close-low-effort/index.js'); console.log('Tests passed');\"",
    "lint": "eslint \".github/actions/**/*.js\" --ext .js --no-error-on-unmatched-pattern",
    "lint:fix": "eslint \".github/actions/**/*.js\" --ext .js --fix --no-error-on-unmatched-pattern"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/substantialcattle5/Octoguard.git"
  },
  "keywords": [
